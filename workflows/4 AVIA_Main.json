    {
      "parameters": {
        "jsCode": "const output = [];\n\nfor (const item of items) {\n  // Intentamos obtener el nombre del archivo desde los binarios\n  const fileName = item.binary?.data?.fileName\n    ? item.binary.data.fileName.toLowerCase()\n    : \"desconocido.pdf\";\n\n  let tipo = \"\";\n\n  if (fileName.includes(\"caracter\")) tipo = \"cuadro\";\n  else if (fileName.includes(\"prescrip\")) tipo = \"prescripciones\";\n  else if (fileName.includes(\"condicion\")) tipo = \"condiciones\";\n  else tipo = \"desconocido\";\n\n  // Combina todas las pÃ¡ginas de texto del PDF en un solo string\n  const fullText = Array.isArray(item.json.text)\n    ? item.json.text.join(\"\\n\")\n    : String(item.json.text || \"\");\n\n  output.push({\n    json: {\n      tipo,\n      fileName,\n      text: fullText\n    }\n  });\n}\n\nreturn output;\n"
      },
